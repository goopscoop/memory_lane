<% if @back %>
  <div class="row ">
    <div class="col-lg-offset-5 col-lg-2">
      <%= link_to "Back To Edit Page", edit_page_path(@page) %>
    </div>

  </div>
<% end %>
<div class="row">
<div class="<%= @current_background.bg_class %>">
  <ul id="list2" class="pageSort show-partial-list">
    <% @partials.each do |e| %>
    <li><%= render partial: "partials/#{e.partial.path}", locals: {p:e.partial} %></li>
    <% end %>
  </ul>
</div>
</div>
<div class="showPageFooter">
